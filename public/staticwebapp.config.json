{
  "routes": [
    {
      "route": "/hub",
      "allowedRoles": ["authenticated"]
    },
    {
      "route": "/hub/*",
      "allowedRoles": ["authenticated"]
    },
    {
      "route": "/login",
      "allowedRoles": ["anonymous", "authenticated"]
    },
    {
      "route": "/discover",
      "redirect": "/discover/",
      "statusCode": 301
    }
  ],
  "responseOverrides": {
    "401": {
      "redirect": "/login",
      "statusCode": 302
    }
  },
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/images/*", "/_next/*", "/favicon.ico", "*.json", "*.xml", "*.txt"]
  },
  "globalHeaders": {
    "X-Frame-Options": "DENY",
    "X-Content-Type-Options": "nosniff"
  }
}

